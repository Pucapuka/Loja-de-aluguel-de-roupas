{
  "name": "loja-aluguel-roupas",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:web\"",
    "dev:backend": "cd backend && node server.js",
    "dev:web": "webpack serve --mode development --host 0.0.0.0",
    "build:web": "webpack --mode production",
    "docker:build": "docker build -t loja-aluguel-roupas .",
    "docker:run": "docker run -p 5000:5000 -v $(pwd)/data:/data --name loja-app loja-aluguel-roupas",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose up -d --build",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:shell": "docker exec -it loja-aluguel-roupas sh",
    "docker:clean": "docker system prune -a -f"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.1",
    "axios": "^1.13.2",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.11.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tauri-apps/cli": "^2.9.6",
    "autoprefixer": "^10.4.23",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.9.1",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.5",
    "invoke": "^0.1.2",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "postcss-nesting": "^13.0.2",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.3.0",
    "webpack": "^5.104.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  }
}
